node('jdk11-mvn3.8.4') {
    stage('git') {
        git branch: 'main', url: 'https://github.com/nagaraju0/spring-petclinic.git'
    }
    stage('build') {
        sh '/usr/local/apache-maven-3.8.4/bin/mvn clean package'
    }
    stage('email') {
        mail bcc: '', body: 'please accept', cc: '', from: '', replyTo: '', subject: 'jenkins job', to: 'nagarajucivil011@gmail.com'
    }
}